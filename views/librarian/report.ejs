<div
  class="flex flex-col w-screen h-screen overflow-auto text-gray-700 bg-gradient-to-tr from-blue-200 via-indigo-200 to-pink-200"
>
  <div
    class="flex items-center flex-shrink-0 w-full h-16 px-10 bg-white bg-opacity-40"
  >
    <a href="/librarian/dashboard">
      <svg
        class="w-8 h-8 text-indigo-600 stroke-current"
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke="currentColor"
      >
        <path
          stroke-linecap="round"
          stroke-linejoin="round"
          stroke-width="2"
          d="M7 21a4 4 0 01-4-4V5a2 2 0 012-2h4a2 2 0 012 2v12a4 4 0 01-4 4zm0 0h12a2 2 0 002-2v-4a2 2 0 00-2-2h-2.343M11 7.343l1.657-1.657a2 2 0 012.828 0l2.829 2.829a2 2 0 010 2.828l-8.486 8.485M7 17h.01"
        />
      </svg>
    </a>
    <div class="ml-10">
      <a
        class="mx-2 text-sm font-semibold text-gray-600 hover:text-indigo-700"
        href="/librarian/transactions"
        >Transctions</a
      >
      <a
        class="mx-2 text-sm font-semibold text-gray-600 hover:text-indigo-700"
        href="#"
        >Report</a
      >
    </div>
  </div>

  <!-- new -->
  <div class="mt-12 mx-16">
    <div class="mb-12 grid gap-y-10 gap-x-6 md:grid-cols-2 xl:grid-cols-4">
      <div
        class="relative flex flex-col bg-clip-border rounded-xl bg-white text-gray-700 shadow-md"
      >
        <div
          class="bg-clip-border mx-4 rounded-xl overflow-hidden bg-gradient-to-tr from-blue-600 to-blue-400 text-white shadow-blue-500/40 shadow-lg absolute -mt-4 grid h-16 w-16 place-items-center"
        >
          <!-- <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" aria-hidden="true" class="w-6 h-6 text-white">
              <path d="M12 7.5a2.25 2.25 0 100 4.5 2.25 2.25 0 000-4.5z"></path>
              <path fill-rule="evenodd" d="M1.5 4.875C1.5 3.839 2.34 3 3.375 3h17.25c1.035 0 1.875.84 1.875 1.875v9.75c0 1.036-.84 1.875-1.875 1.875H3.375A1.875 1.875 0 011.5 14.625v-9.75zM8.25 9.75a3.75 3.75 0 117.5 0 3.75 3.75 0 01-7.5 0zM18.75 9a.75.75 0 00-.75.75v.008c0 .414.336.75.75.75h.008a.75.75 0 00.75-.75V9.75a.75.75 0 00-.75-.75h-.008zM4.5 9.75A.75.75 0 015.25 9h.008a.75.75 0 01.75.75v.008a.75.75 0 01-.75.75H5.25a.75.75 0 01-.75-.75V9.75z" clip-rule="evenodd"></path>
              <path d="M2.25 18a.75.75 0 000 1.5c5.4 0 10.63.722 15.6 2.075 1.19.324 2.4-.558 2.4-1.82V18.75a.75.75 0 00-.75-.75H2.25z"></path>
            </svg> -->
        </div>
        <div class="p-4 text-right">
          <p
            class="block antialiased font-sans text-sm leading-normal font-normal text-blue-gray-600"
          >
            Total users
          </p>
          <h4
            class="block antialiased tracking-normal font-sans text-2xl font-semibold leading-snug text-blue-gray-900"
          >
            <%= users.length %>
          </h4>
        </div>
        <div class="border-t border-blue-gray-50 p-4">
          
        </div>
      </div>
      <div
        class="relative flex flex-col bg-clip-border rounded-xl bg-white text-gray-700 shadow-md"
      >
        <div
          class="bg-clip-border mx-4 rounded-xl overflow-hidden bg-gradient-to-tr from-pink-600 to-pink-400 text-white shadow-pink-500/40 shadow-lg absolute -mt-4 grid h-16 w-16 place-items-center"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 24 24"
            fill="currentColor"
            aria-hidden="true"
            class="w-6 h-6 text-white"
          >
            <path
              fill-rule="evenodd"
              d="M7.5 6a4.5 4.5 0 119 0 4.5 4.5 0 01-9 0zM3.751 20.105a8.25 8.25 0 0116.498 0 .75.75 0 01-.437.695A18.683 18.683 0 0112 22.5c-2.786 0-5.433-.608-7.812-1.7a.75.75 0 01-.437-.695z"
              clip-rule="evenodd"
            ></path>
          </svg>
        </div>
        <div class="p-4 text-right">
          <p
            class="block antialiased font-sans text-sm leading-normal font-normal text-blue-gray-600"
          >
            Total books
          </p>
          <h4
            class="block antialiased tracking-normal font-sans text-2xl font-semibold leading-snug text-blue-gray-900"
          >
            <%= books.length %>
          </h4>
        </div>
        <div class="border-t border-blue-gray-50 p-4">
          
        </div>
      </div>
      <div
        class="relative flex flex-col bg-clip-border rounded-xl bg-white text-gray-700 shadow-md"
      >
        <div
          class="bg-clip-border mx-4 rounded-xl overflow-hidden bg-gradient-to-tr from-green-600 to-green-400 text-white shadow-green-500/40 shadow-lg absolute -mt-4 grid h-16 w-16 place-items-center"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 24 24"
            fill="currentColor"
            aria-hidden="true"
            class="w-6 h-6 text-white"
          >
            <path
              d="M6.25 6.375a4.125 4.125 0 118.25 0 4.125 4.125 0 01-8.25 0zM3.25 19.125a7.125 7.125 0 0114.25 0v.003l-.001.119a.75.75 0 01-.363.63 13.067 13.067 0 01-6.761 1.873c-2.472 0-4.786-.684-6.76-1.873a.75.75 0 01-.364-.63l-.001-.122zM19.75 7.5a.75.75 0 00-1.5 0v2.25H16a.75.75 0 000 1.5h2.25v2.25a.75.75 0 001.5 0v-2.25H22a.75.75 0 000-1.5h-2.25V7.5z"
            ></path>
          </svg>
        </div>
        <div class="p-4 text-right">
          <p
            class="block antialiased font-sans text-sm leading-normal font-normal text-blue-gray-600"
          >
            Total Unavailable books
          </p>
          <h4
            class="block antialiased tracking-normal font-sans text-2xl font-semibold leading-snug text-blue-gray-900"
          >
            <%= unavailableBooks.length %>
          </h4>
        </div>
        <div class="border-t border-blue-gray-50 p-4">
          
        </div>
      </div>
      <div
        class="relative flex flex-col bg-clip-border rounded-xl bg-white text-gray-700 shadow-md"
      >
        <div
          class="bg-clip-border mx-4 rounded-xl overflow-hidden bg-gradient-to-tr from-orange-600 to-orange-400 text-white shadow-orange-500/40 shadow-lg absolute -mt-4 grid h-16 w-16 place-items-center"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 24 24"
            fill="currentColor"
            aria-hidden="true"
            class="w-6 h-6 text-white"
          >
            <path
              d="M18.375 2.25c-1.035 0-1.875.84-1.875 1.875v15.75c0 1.035.84 1.875 1.875 1.875h.75c1.035 0 1.875-.84 1.875-1.875V4.125c0-1.036-.84-1.875-1.875-1.875h-.75zM9.75 8.625c0-1.036.84-1.875 1.875-1.875h.75c1.036 0 1.875.84 1.875 1.875v11.25c0 1.035-.84 1.875-1.875 1.875h-.75a1.875 1.875 0 01-1.875-1.875V8.625zM3 13.125c0-1.036.84-1.875 1.875-1.875h.75c1.036 0 1.875.84 1.875 1.875v6.75c0 1.035-.84 1.875-1.875 1.875h-.75A1.875 1.875 0 013 19.875v-6.75z"
            ></path>
          </svg>
        </div>
        <div class="p-4 text-right">
          <p
            class="block antialiased font-sans text-sm leading-normal font-normal text-blue-gray-600"
          >
            Total transactions
          </p>
          <h4
            class="block antialiased tracking-normal font-sans text-2xl font-semibold leading-snug text-blue-gray-900"
          >
            <%= loans.length %>
          </h4>
        </div>
        <div class="border-t border-blue-gray-50 p-4">
         
        </div>
      </div>
    </div>

    <div class="w-full">
      <div class="px-10 mt-6">
        <div class="overflow-hidden">
          <table class="min-w-full bg-white border border-gray-300 rounded-lg">
            <thead class="bg-gray-100">
              <tr>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  #
                </th>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  User ID
                </th>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  User Name
                </th>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  Title
                </th>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  Author
                </th>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  Loan Date
                </th>
                <th
                  class="px-6 py-4 text-left text-sm font-medium text-gray-900"
                >
                  Return Date
                </th>
              </tr>
            </thead>
            <tbody>
              <% if (loans.length > 0) { %> <% loans.forEach((loan, index) => {
              %>
              <tr
                class="<%= index % 2 === 0 ? 'bg-gray-50' : 'bg-white' %> border-b"
              >
                <td class="px-6 py-4 text-gray-900"><%= index + 1 %></td>
                <td class="px-6 py-4 text-gray-900"><%= loan.user_id %></td>
                <td class="px-6 py-4 text-gray-900"><%= loan.user_name %></td>
                <td class="px-6 py-4 text-gray-900"><%= loan.title %></td>
                <td class="px-6 py-4 text-gray-900"><%= loan.author %></td>
                <td class="px-6 py-4 text-gray-900">
                  <%= new Date(loan.loan_date).toLocaleDateString() %>
                </td>
                <td class="px-6 py-4 text-gray-900">
                  <%= loan.return_date ? new
                  Date(loan.return_date).toLocaleDateString() : 'Not Returned'
                  %>
                </td>
              </tr>
              <% }); %> <% } else { %>
              <tr>
                <td colspan="8" class="text-center px-6 py-4 text-gray-700">
                  No loan history available.
                </td>
              </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <!-- end -->
</div>
